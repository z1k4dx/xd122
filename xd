/* <![CDATA[ */
var JetEngineSettings = {"ajaxurl":"http:\/\/suap.policia.sanluis.gov.ar\/wp-admin\/admin-ajax.php","ajaxlisting":"http:\/\/suap.policia.sanluis.gov.ar\/?datos-personal-pol=31212899&nocache=1730172285","mapPopupTimeout":"400"};
